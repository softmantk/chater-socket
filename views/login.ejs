<%include ./header.ejs%>
<form class="form-auth" action="/auth/login" method="post">
    <h2>Log In</h2>
    <p class="text-warning">
        <%= error_message %>
    </p>
    <input type="username"  placeholder="Username" class="form-control" name="username"><br>
    <input type="password" placeholder="Password" class="form-control" name="password"><br>
    <input type="submit" value="Log in" class="btn btn-primary" />
</form>
